# Deploy API credentials
# Copy this file to .env.local and fill in the values.
# This file should NOT be committed to git.

DEPLOY_API_URL=https://deploy.kubernetes.crianex.com
DEPLOY_API_KEY=your-deploy-api-key-here

# Supabase credentials (for Kubernetes deployment)
SUPABASE_URL=https://your-project.supabase.co
SUPABASE_SERVICE_ROLE_KEY=your-service-role-key
SUPABASE_ANON_KEY=your-anon-key

# RAGFlow credentials (for AI Agent)
RAGFLOW_API_KEY=your-ragflow-api-key
RAGFLOW_URL=https://your-ragflow-url

# Optional
# DEPLOY_REGISTRY=registry.kubernetes.crianex.com
# DEPLOY_API_TIMEOUT=30
