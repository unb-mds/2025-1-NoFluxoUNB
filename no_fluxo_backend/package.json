{
  "name": "no-fluxo-backend",
  "version": "1.0.0",
  "description": "Backend do projeto No Fluxo com TypeScript, Express.js e Supabase",
  "main": "dist/index.js",
  "scripts": {
    "setup-tesseract": "node scripts/setup-tesseract.js",
    "build": "pip install -r AI-agent/requirements.txt && pip install -r parse-pdf/requirements.txt && tsc",
    "build-docker": "pip install --user -r AI-agent/requirements.txt && pip install --user -r parse-pdf/requirements.txt && tsc",
    "build-and-start": "npm run build && npm run start",
    "start": "node dist/index.js",
    "dev": "pip install -r AI-agent/requirements.txt && pip install -r parse-pdf/requirements.txt && nodemon src/index.ts",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "lint": "eslint src/**/*.ts tests-ts/**/*.ts",
    "lint:fix": "eslint src/**/*.ts tests-ts/**/*.ts --fix",
    "type-check": "tsc --noEmit"
  },
  "keywords": [
    "typescript",
    "express",
    "supabase",
    "backend",
    "api"
  ],
  "author": "Squad 03",
  "license": "MIT",
  "dependencies": {
    "@supabase/supabase-js": "^2.38.4",
    "axios": "^1.10.0",
    "body-parser": "^1.20.2",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-fileupload": "^1.4.3",
    "express-ws": "^5.0.2",
    "form-data": "^4.0.3",
    "helmet": "^7.1.0",
    "morgan": "^1.10.0",
    "winston": "^3.12.0"
  },
  "devDependencies": {
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/express-fileupload": "^1.4.1",
    "@types/form-data": "^2.2.1",
    "@types/jest": "^29.5.8",
    "@types/morgan": "^1.9.9",
    "@types/node": "^20.10.0",
    "@typescript-eslint/eslint-plugin": "^6.21.0",
    "@typescript-eslint/parser": "^6.21.0",
    "eslint": "^8.54.0",
    "jest": "^29.7.0",
    "nodemon": "^3.0.2",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.1",
    "typescript": "^5.3.2"
  },
  "jest": {
    "preset": "ts-jest",
    "testEnvironment": "node",
    "testMatch": [
      "<rootDir>/tests-ts/**/*.test.ts",
      "<rootDir>/src/**/__tests__/**/*.test.ts"
    ],
    "collectCoverageFrom": [
      "src/**/*.ts",
      "!src/**/*.d.ts"
    ],
    "coverageDirectory": "coverage",
    "coverageReporters": [
      "text",
      "lcov",
      "html"
    ],
    "setupFilesAfterEnv": [
      "<rootDir>/jest.setup.js"
    ]
  }
}
