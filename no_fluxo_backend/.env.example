# NoFluxo Backend Docker Environment Configuration
# Copy this to .env and fill in your actual values

# === SUPABASE CONFIGURATION ===
SUPABASE_URL=your_supabase_url_here
SUPABASE_SERVICE_ROLE_KEY=your_supabase_service_role_key_here

# === AI AGENT CONFIGURATION ===
RAGFLOW_API_KEY=your_ragflow_api_key_here
RAGFLOW_BASE_URL=your_ragflow_base_url_here
RAGFLOW_AGENT_ID=your_ragflow_agent_id_here

# === DOCKER AUTO-UPDATE CONFIGURATION ===
# Git credentials for auto-updating the repository in Docker
GIT_USERNAME=your_github_username
GIT_TOKEN=your_github_personal_access_token

# Branch to monitor for updates (default: main)
GIT_BRANCH=main

# Fork repository URL for automatic cloning (optional)
# If specified, the fork will be cloned inside the container and updated automatically
# Example: https://github.com/yourusername/2025-1-NoFluxoUNB.git
FORK_URL=

# === NODE.JS CONFIGURATION ===
NODE_ENV=production
PORT=3325

# === AI AGENT PORT ===
AI_AGENT_PORT=4652

# === PDF PARSER CONFIGURATION ===
PDF_PARSER_PORT=3001

# === LOGGING CONFIGURATION ===
LOG_LEVEL=info 